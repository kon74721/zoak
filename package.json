{
  "name": "cesium-webpack-boilerplate",
  "version": "1.1.0",
  "description": "A boilerplate for cesiumjs",
  "main": "app.js",
  "scripts": {
    "clean": "mkdirp build & rimraf build/*",
    "build": "npm run clean && cpr node_modules/cesium/Build/Cesium build && webpack --config webpack.config.js --progress",
    "build-release": "npm run build -- --optimize-minimize",
    "start": "webpack-dev-server --config webpack.config.js --progress --display-error-details"
  },
  "author": "Maxime Warnier",
  "license": "MIT",
  "dependencies": {
    "cesium": "^1.32.1"
  },
  "devDependencies": {
    "cpr": "^1.1.2",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
